# ESPHome Secrets Template
# Copy this file to secrets.yaml and fill in your actual values
# secrets.yaml should be added to .gitignore

# WiFi credentials
wifi_ssid: "YourWiFiSSID"
wifi_password: "YourWiFiPassword"

# API encryption key (generate with: openssl rand -base64 32)
api_encryption_key: "your-32-character-api-encryption-key-here"

# OTA password
ota_password: "your-ota-password-here"
